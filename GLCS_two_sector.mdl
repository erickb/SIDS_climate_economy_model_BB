{UTF-8}
average interest rate on debt[scenario]= INTEG (
	new interest[scenario]-old interest[scenario],
		interest rate on new loans)
	~	
	~		|

investment and rebuilding[scenario]=
	K[scenario]*(investment rate[scenario] + IF THEN ELSE(rebuild after climate damage,SMOOTHI\
		(climate damage,1,0),0))
	~	
	~	This is a "full recovery" model for climate damage -- it is repaired in \
		the subsequent time step.
	|

change in capital stock[scenario]=
	K[scenario]/SMOOTH(K[scenario],TIME STEP) - 1
	~	
	~		|

change in exports[scenario]=
	export growth rate[scenario] * X[scenario]
	~	
	~		|

change in gdp[scenario]=
	gdp[scenario]/SMOOTH(gdp[scenario],TIME STEP) - 1
	~	
	~		|

change in utilization[scenario]=
	marginal change in utilization[scenario] * utilization ratio[scenario]
	~	
	~		|

marginal change in utilization[scenario]=
	change in gdp[scenario] - change in capital stock[scenario]
	~	
	~		|

gdp exports direct[scenario]=
	X prod[scenario] * px norm
	~	
	~		|

gdp exports indirect[scenario]=
	common multiplier * (wage share domestic * gdp coeff xd * pd norm + wage share export\
		 * gdp coeff xx * px norm) * X prod[scenario]
	~	
	~		|

gdp growth rate[scenario]=
	gdp[scenario]/SMOOTH(gdp[scenario],TIME STEP) - 1
	~	
	~		|

X prod[scenario]=
	SMOOTHI(X[scenario],X prod smoothing,init X prod) * (1 + X prod smoothing * (SMOOTH(\
		X[scenario],2)-SMOOTH(X[scenario],1))/(2*SMOOTH(X[scenario],1) - SMOOTH(X[scenario]\
		,2
	)))
	~	
	~	base + prediction
	|

gdp per capita[scenario]=
	1000*gdp[scenario]/N
	~	
	~		|

imports[scenario]=
	(price level of gdp/price of imports) * coeff my * gdp[scenario]
	~	
	~		|

net new external liabilities[scenario]=
	price of imports * imports[scenario] - price of exports * X[scenario]
	~	
	~		|

D[scenario]= INTEG (
	SMOOTH(interest on debt[scenario],TIME STEP)+net new loans[scenario],
		initial external debt)
	~	
	~		|

debt to gdp ratio[scenario]=
	exchange rate * D[scenario]/(price level of gdp * gdp[scenario])
	~	
	~		|

depreciation[scenario]=
	(depreciation rate + climate damage) * K[scenario]
	~	
	~		|

new interest[scenario]=
	interest rate on new loans * new debt[scenario]/D[scenario]
	~	
	~		|

phi exponent[scenario]= INTEG (
	incr phi exponent[scenario],
		0)
	~	
	~		|

export growth rate[scenario]=
	RANDOM NORMAL(-0.5, 0.5 , mean export growth rate[scenario] , export growth standard deviation\
		[scenario] , 0)
	~	
	~		|

X[scenario]= INTEG (
	change in exports[scenario],
		initial exports)
	~	
	~		|

previous utilization deviation[scenario]=
	SMOOTHI(utilization ratio[scenario], TIME STEP,initial utilization) - 1
	~	
	~		|

gdp[scenario]=
	gdp exports direct[scenario] + gdp basic consumption direct + gdp exports indirect[scenario\
		] + gdp basic consumption indirect
	~	
	~		|

K[scenario]= INTEG (
	investment and rebuilding[scenario]-depreciation[scenario],
		initial capital)
	~	
	~		|

incr phi exponent[scenario]=
	investment debt term[scenario] + investment utilization term[scenario] - foreign domestic investment gap
	~	
	~		|

investment debt term[scenario]=
	coeff id * (debt to gdp ratio[scenario] - reference debt to gdp level)
	~	
	~		|

old interest[scenario]=
	average interest rate on debt[scenario] * new debt[scenario]/D[scenario]
	~	
	~		|

investment utilization term[scenario]=
	coeff iu  * previous utilization deviation[scenario]
	~	
	~		|

interest on debt[scenario]=
	average interest rate on debt[scenario] * D[scenario]
	~	
	~		|

phi[scenario]=
	initial phi/(initial phi + (1-initial phi)*exp(phi exponent[scenario]))
	~	
	~		|

new debt[scenario]=
	max(0,net new loans[scenario])
	~	
	~		|

smoothed gdp growth rate[scenario]=
	SMOOTH(gdp growth rate[scenario],gdp growth smoothing)
	~	
	~		|

scenario:
	CS1,CS2,CS3,CS4
	~	
	~		|

net saving[scenario]=
	net new external liabilities[scenario] - price of investment * investment and rebuilding\
		[scenario]
	~	
	~		|

utilization ratio[scenario]= INTEG (
	change in utilization[scenario],
		initial utilization)
	~	
	~		|

investment rate[scenario]=
	max(0,(smoothed gdp growth rate[scenario] + base investment rate + (1 - phi[scenario\
		]) * investment utilization term[scenario]
	 - phi[scenario] * investment debt term[scenario]))
	~	
	~		|

net new loans[scenario]=
	interest bearing fraction*(phi[scenario] * net saving[scenario] + (1-phi[scenario]) \
		* net new external liabilities[scenario])/exchange rate
	~	
	~		|

rebuild after climate damage=
	GET XLS CONSTANTS( '?params' , 'Switches' , 'C3' )
	~	
	~		|

climate beta=
	GET XLS CONSTANTS( '?params' , 'Storm Damage' , 'D11' )
	~	
	~		|

minimum return period:=
	GET XLS DATA('?params', 'Storm Damage' , '3' , 'C4' )
	~	
	~		|

p climate=
	RANDOM UNIFORM(0, 1 , 0 )
	~	
	~		|

climate damage=
	IF THEN ELSE(p climate * minimum return period < 1, 1 - (p climate * minimum return period\
		)^(1/climate beta), 0 )
	~	
	~		|

X prod smoothing:=
	GET XLS DATA('?params', 'GDP', '3' , 'C13' )
	~	
	~		|

production determinant:=
	GET XLS DATA('?params', 'GDP', '3' , 'C4' )
	~	
	~		|

mean export growth rate[scenario]:INTERPOLATE::=
	GET XLS DATA('?params', 'Exports' , '3' , 'D4' )
	~	
	~		|

initial exports=
	GET XLS CONSTANTS( '?params' , 'Initial Values' , 'C2' )
	~	
	~	K/1.96 = 1.6e+010 (use with N = 0)
		Data: 3.6e+009
	|

marginal expenditure on domestic goods:=
	GET XLS DATA('?params', 'GDP', '3' , 'C5' )
	~	
	~		|

NOISE SEED=
	1324
	~	
	~		|

export growth standard deviation[scenario]:INTERPOLATE::=
	GET XLS DATA('?params', 'Exports' , '3' , 'D8' )
	~	
	~		|

real basic consumption=
	(1 - marginal expenditure on domestic goods) * minimum consumption level * N
	~	
	~		|

common multiplier=
	marginal expenditure on domestic goods/production determinant
	~	
	~		|

gdp basic consumption direct=
	real basic consumption * pd norm
	~	
	~		|

gdp basic consumption indirect=
	common multiplier * (wage share domestic * gdp coeff nd * pd norm + wage share export\
		 * gdp coeff nx * px norm) * real basic consumption
	~	
	~		|

gdp coeff nd:=
	GET XLS DATA('?params', 'GDP', '3' , 'C11' )
	~	
	~		|

gdp coeff nx:=
	GET XLS DATA('?params', 'GDP', '3' , 'C12' )
	~	
	~		|

gdp coeff xx:=
	GET XLS DATA('?params', 'GDP', '3' , 'C10' )
	~	
	~		|

minimum consumption level:=
	GET XLS DATA('?params', 'GDP', '3' , 'C6' )
	~	
	~		|

px norm=
	price of exports/price level of gdp
	~	
	~		|

gdp coeff xd:=
	GET XLS DATA('?params', 'GDP', '3' , 'C9' )
	~	
	~		|

wage share domestic:=
	GET XLS DATA('?params', 'GDP', '3' , 'C7' )
	~	
	~		|

foreign domestic investment gap:=
	GET XLS DATA('?params', 'Investment and Saving', '4' , 'C9' )
	~	
	~		|

pd norm=
	price of consumption/price level of gdp
	~	
	~		|

reference debt to gdp level:INTERPOLATE::=
	GET XLS DATA('?params', 'Investment and Saving', '12' , 'C15' )
	~	
	~		|

wage share export:=
	GET XLS DATA('?params', 'GDP', '3' , 'C8' )
	~	
	~		|

init X prod=
	GET XLS CONSTANTS( '?params' , 'Initial Values' , 'C7' )
	~	
	~		|

interest bearing fraction:INTERPOLATE::=
	GET XLS DATA('?params', 'Investment and Saving', '12' , 'C14' )
	~	
	~		|

gdp growth smoothing:=
	GET XLS DATA('?params', 'GDP', '3' , 'C14' )
	~	
	~		|

initial phi=
	GET XLS CONSTANTS( '?params' , 'Initial Values' , 'C8' )
	~	
	~		|

initial utilization=
	GET XLS CONSTANTS( '?params' , 'Initial Values' , 'C6' )
	~	
	~		|

coeff id:=
	GET XLS DATA('?params', 'Investment and Saving', '4' , 'C8' )
	~	
	~		|

coeff iu:INTERPOLATE::=
	GET XLS DATA('?params', 'Investment and Saving', '4' , 'C7' )
	~	
	~		|

coeff my:=
	GET XLS DATA('?params', 'imports', '3' , 'C4' )
	~	
	~		|

price level of gdp:=
	GET XLS DATA('?params', 'Prices', '3' , 'D9' )
	~	
	~		|

price of consumption:=
	GET XLS DATA('?params', 'Prices', '3' , 'D6' )
	~	
	~		|

exchange rate:=
	GET XLS DATA('?params', 'Prices', '3' , 'D4' )
	~	
	~		|

price of investment:=
	GET XLS DATA('?params', 'Prices', '3' , 'D5' )
	~	
	~		|

price of exports:=
	GET XLS DATA('?params', 'Prices', '3' , 'D8' )
	~	
	~		|

price of imports:=
	GET XLS DATA('?params', 'Prices', '3' , 'D7' )
	~	
	~		|

base investment rate:INTERPOLATE::=
	GET XLS DATA('?params', 'Investment and Saving', '4' , 'C5' )
	~	
	~		|

crude birth rate:=
	GET XLS DATA('?params', 'Demographics' , '3' , 'C5' )
	~	
	~		|

crude death rate:=
	GET XLS DATA('?params', 'Demographics' , '3' , 'C6' )
	~	
	~		|

depreciation rate:INTERPOLATE::=
	GET XLS DATA('?params', 'Investment and Saving', '4' , 'C6' )
	~	
	~		|

initial capital=
	GET XLS CONSTANTS( '?params' , 'Initial Values' , 'C3' )
	~	
	~		|

initial external debt=
	GET XLS CONSTANTS( '?params' , 'Initial Values' , 'C4' )
	~	
	~		|

initial population=
	GET XLS CONSTANTS( '?params' , 'Initial Values' , 'C5' )
	~	
	~		|

interest rate on new loans:INTERPOLATE::=
	GET XLS DATA('?params', 'Investment and Saving', '12' , 'C13' )
	~	
	~		|

N= INTEG (
	population increase-population decrease,
		initial population)
	~	
	~		|

net migration rate:=
	GET XLS DATA('?params', 'Demographics' , '3' , 'C4' )
	~	
	~		|

population decrease=
	N * crude death rate/1000
	~	
	~		|

population increase=
	N * (crude birth rate + net migration rate)/1000
	~	
	~		|

